Program:
  '@quiz' description=Sentence
    questions+=Question
;

Question:
  '?' question=Sentence
   options+=Option
;

QuestionText:
  (Sentence | Modifier)+
;

Modifier:
  '@blank' 
;

Option:
  otype=OptionType otext=Sentence
;

OptionType:
  ('+' | '-')
;

Comment:
  /\/\/.*$/
;

Sentence:
  /.+/
;
